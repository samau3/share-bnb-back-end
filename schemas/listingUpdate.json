{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/listingNew.schema.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "street": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "city": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "state": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "country": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "description": {
      "type": "string"
    },
    "photoUrl": {
      "type": "string",
      "format": "uri"
    },
    "price": {
      "type": "integer",
      "minimum": 0
    },
    "username": {
      "type": "string",
      "maxLength": 25
    }
  },
  "additionalProperties": false,
  "required": []
}